{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "MassTransit": "Debug",
      "AzureFunction.MassTransit.Demos.ServiceBus": "Information"
    }
  },
  "extensions": {
    "RabbitMQ": {
      "prefetchCount": 100,
      "queueName": "conferences",
      "connectionString": "amqp://guest:guest@host.docker.internal:5672",
      "port": 10
    }
  }
}